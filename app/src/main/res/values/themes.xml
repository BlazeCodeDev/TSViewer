<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.TSViewer" parent="Theme.Material3.DayNight">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/material_dynamic_primary40</item>
        <item name="colorPrimaryVariant">@color/material_dynamic_secondary40</item>
        <item name="colorOnPrimary">@color/material_dynamic_primary100</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/material_dynamic_neutral10</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.TSViewer.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.TSViewer.AppBarOverlay" parent="ThemeOverlay.Material3" />

    <style name="Theme.TSViewer.PopupOverlay" parent="ThemeOverlay.Material3" />
</resources>